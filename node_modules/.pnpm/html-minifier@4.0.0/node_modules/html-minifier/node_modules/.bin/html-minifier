#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/Users/vivaldi/Documents/GitHub.nosync/UI/node_modules/.pnpm/html-minifier@4.0.0/node_modules/html-minifier/node_modules:/Users/vivaldi/Documents/GitHub.nosync/UI/node_modules/.pnpm/html-minifier@4.0.0/node_modules:/Users/vivaldi/Documents/GitHub.nosync/UI/node_modules/.pnpm/node_modules:/Users/vivaldi/Documents/GitHub.nosync/UI/node_modules:/Users/vivaldi/Documents/GitHub.nosync/node_modules:/Users/vivaldi/Documents/node_modules:/Users/vivaldi/node_modules:/Users/node_modules:/node_modules"
else
  export NODE_PATH="$NODE_PATH:/Users/vivaldi/Documents/GitHub.nosync/UI/node_modules/.pnpm/html-minifier@4.0.0/node_modules/html-minifier/node_modules:/Users/vivaldi/Documents/GitHub.nosync/UI/node_modules/.pnpm/html-minifier@4.0.0/node_modules:/Users/vivaldi/Documents/GitHub.nosync/UI/node_modules/.pnpm/node_modules:/Users/vivaldi/Documents/GitHub.nosync/UI/node_modules:/Users/vivaldi/Documents/GitHub.nosync/node_modules:/Users/vivaldi/Documents/node_modules:/Users/vivaldi/node_modules:/Users/node_modules:/node_modules"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../../cli.js" "$@"
else
  exec node  "$basedir/../../cli.js" "$@"
fi
